<launch>
    <arg name="map_size_x" value="20.0"/>
    <arg name="map_size_y" value="10.0"/>
    <arg name="map_size_z" value=" 5.0"/>

    <node pkg ="map_generator" name ="map_generator" type ="simple_dynamic_map" output = "screen">
        
        <param name="map/x_size"     value="$(arg map_size_x)" />
        <param name="map/y_size"     value="$(arg map_size_y)" />
        <param name="map/z_size"     value="$(arg map_size_z)" />
        <param name="map/resolution" value="0.1"/>        
        <param name="map/frame_id"     value="world" />
        <param name="map/seed" value="4"/>
        <param name="map/cylinder_num"    value="20"/>

        <param name="obstacle/upper_vel" value="1.0"/>


        <param name="cylinder/lower_weight" value="0.2"/>
        <param name="cylinder/upper_weight" value="0.9"/>
        <param name="cylinder/lower_height" value="4.0"/>
        <param name="cylinder/upper_height" value="4.0"/>     
        <param name="cylinder/upper_vel" value="0.1"/>
        <param name="cylinder/set_cylinder" value="false"/>
        <!-- 0 satic , 1: XAxis, 2: Y:Axis, 3:Random-->
        <param name="cylinder/move_mode" value="2"/>
        <param name="cylinder/pos_mode" value="1"/>   

        <param name="circle/radius_l" value="0.7"/>        
        <param name="circle/radius_h" value="2.5"/>        
        <param name="circle/thickness" value="0.1"/>        
        <param name="circle/dr" value="0.3"/>        
        <param name="circle/theta" value="1.2"/>        

        <param name="sensing/range"  value="0.7"/>
        <param name="sensing/rate"   value="10.0"/>   

    </node>

    <include file="$(find ego_planner)/launch/rviz.launch"/>


</launch>